SG_OAKSRCLIB=$(_COMMONOAKROOT)\lib\$(_CPUINDPATH)
SG_OAKSRCTGT=$(_COMMONOAKROOT)\target\$(_CPUINDPATH)
SG_SDKSRCLIB=$(_COMMONSDKROOT)\lib\$(_CPUINDPATH)
SG_SDKLIBDIR=$(_PROJECTROOT)\cesysgen\sdk\lib\$(_CPUINDPATH)
SG_OAKLIBDIR=$(_PROJECTROOT)\cesysgen\oak\lib\$(_CPUINDPATH)
SG_OAKTGTDIR=$(_PROJECTROOT)\cesysgen\oak\target\$(_CPUINDPATH)

!IF ("$(_TGTOS)" == "CE")
PREPROCESSDEFFILE=1
DEFFILE=LANUSB.def
TARGETDEFNAME=LANUSB
TARGETNAME=LANUSB
TARGETTYPE=DYNLINK
!ENDIF

CDEFINES=$(C_DEFINES)

TARGETLIBS=$(SG_OAKSRCLIB)\LANUSB.lib \
           $(_SYSGENOAKROOT)\lib\$(_CPUINDPATH)\ceddk.lib \
           $(SG_SDKLIBDIR)\ndis.lib \
           $(SG_SDKLIBDIR)\coredll.lib

